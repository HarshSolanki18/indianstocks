  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop:api": "ENABLE_GATSBY_REFRESH_ENDPOINT=true API=true gatsby develop -p 8085 ",
